                CFrameMon = CFrame.new(-5253.6767578125, 513.6262817382812, -2217.569580078125)
                CFrameMon = CFrame.new(-5264.47314, 393.427216, -2199.49634, -0.858525395, -1.04187032e-08, 0.51277107, -4.53612934e-08, 1, -5.56293465e-08, -0.51277107, -7.10191657e-08, -0.858525395)
                CFrameQuest = CFrame.new(-4840.64208984375, 717.6693725585938, -2621.085693359375)
                CFrameQuest = CFrame.new(-4840.64209, 717.669373, -2621.08569, -0.433127195, -5.06357765e-08, 0.901332796, -4.84701452e-08, 1, 3.28868985e-08, -0.901332796, -2.94435232e-08, -0.433127195)
