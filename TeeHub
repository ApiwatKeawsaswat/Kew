                CFrameQuest = CFrame.new(-4840.21973, 718.149597, -2620.48413, -0.910135567, -7.65807529e-10, -0.414310575, -1.16961063e-09, 1, 7.20948801e-10, 0.414310575, 1.14074328e-09, -0.910135567)
